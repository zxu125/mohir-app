model RefreshTokens {
    id        String   @unique
    tokenHash String   @map("token_hash")
    revoked   Boolean  @default(false)
    expiresAt BigInt   @map("expires_at")
    userId    Int
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt

    user User @relation(fields: [userId], references: [id])

    @@map("refresh_tokens")
}
